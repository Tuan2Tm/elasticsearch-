# Test Environment Configuration
COMPOSE_PROJECT_NAME=elastic-local-test

# Elasticsearch Configuration
ELASTICSEARCH_IMAGE=docker.elastic.co/elasticsearch/elasticsearch:8.15.0
ELASTICSEARCH_CONTAINER_NAME=elasticsearch-test
ELASTICSEARCH_PORT_HTTP=9201
ELASTICSEARCH_PORT_TRANSPORT=9301
ELASTIC_PASSWORD=test_password_123
ES_JAVA_OPTS=-Xms512m -Xmx512m
ELASTICSEARCH_DATA_VOLUME=esdata-test

# Kibana Configuration
KIBANA_IMAGE=docker.elastic.co/kibana/kibana:8.15.0
KIBANA_CONTAINER_NAME=kibana-test
KIBANA_PORT=5602
KIBANA_SERVERNAME=kibana-test
KIBANA_SERVER_HOST=0.0.0.0

# Security Configuration
X_PACK_SECURITY_ENABLED=true
X_PACK_SECURITY_HTTP_SSL_ENABLED=false
X_PACK_SECURITY_TRANSPORT_SSL_ENABLED=false
DISCOVERY_TYPE=single-node
